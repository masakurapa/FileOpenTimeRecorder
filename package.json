{
	"name": "fileopenrecorder",
	"displayName": "FileOpenRecorder",
	"description": "recording file open times",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:fileopenrecorder.start",
		"onCommand:fileopenrecorder.stop"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "fileopenrecorder.start",
				"title": "FileOpenRecorder: Start Recoding"
			},
			{
				"command": "fileopenrecorder.stop",
				"title": "FileOpenRecorder: Stop Recoding"
			}
		],
		"configuration":[
			{
				"title": "Aggregation Directories",
				"properties": {
					"fileopenrecorder.aggrigationDirectories": {
						"type": "array",
						"default": [],
						"description": "relative path from root paths",
						"scope": "window"
					}
				}
			},
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"tslint": "^6.1.2",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}
